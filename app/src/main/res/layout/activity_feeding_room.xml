<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <android.support.v7.widget.Toolbar
        android:id="@+id/add_donor_layout_layout_my_toolbar"
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:layout_alignParentTop="true"
        android:background="#FBEDEA"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

        <ImageView
            android:id="@+id/login_layout_header_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/logo_login" />
    </android.support.v7.widget.Toolbar>


    <TextView
        android:id="@+id/feedingRoom_textView_title"
        android:layout_width="match_parent"
        android:layout_height="19dp"
        android:layout_marginTop="55dp"
        android:background="#F4F4F4"
        android:gravity="center"
        android:text="수유실 정보"
        android:textColor="#3ECFD0"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/feedingRoom_layout_Button_roadSearch_Button"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/feeding_room_button"
        android:text="길찾기"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="@+id/feedingRoom_layout_textView_feedingRoomAddress"
        app:layout_constraintTop_toBottomOf="@+id/feedingRoom_layout_textView_feedingRoomAddress" />


    <Button
        android:id="@+id/feedingRoom_layout_Button_donorAdd_Button"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/feeding_room_button"
        android:text="후기보기"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/feedingRoom_layout_Button_roadSearch_Button"
        app:layout_constraintStart_toEndOf="@+id/feedingRoom_layout_Button_roadSearch_Button"
        app:layout_constraintTop_toTopOf="@+id/feedingRoom_layout_Button_roadSearch_Button" />


    <View
        android:id="@+id/login_layout_view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:background="#999999"
        app:layout_constraintBottom_toTopOf="@+id/donor_layout_donor_listView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />


    <android.support.constraint.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="206dp" />

    <ListView
        android:id="@+id/donor_layout_donor_listView"
        android:layout_width="match_parent"
        android:layout_height="340dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_layout_view2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_marginStart="8dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/feedingRoom_layout_Button_donorAdd_Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/feedingRoom_layout_Button_donorAdd_Button"
        app:layout_constraintTop_toBottomOf="@+id/feedingRoom_textView_title"
        app:srcCompat="@drawable/feeding_room_img" />

    <TextView
        android:id="@+id/feedingRoom_layout_textView_feedingRoomName"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="수유실 이름"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedingRoom_textView_title" />

    <TextView
        android:id="@+id/feedingRoom_layout_textView_feedingRoomAddress"
        android:layout_width="200sp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="수유실 주소"
        app:layout_constraintStart_toStartOf="@+id/feedingRoom_layout_textView_feedingRoomName"
        app:layout_constraintTop_toBottomOf="@+id/feedingRoom_layout_textView_feedingRoomName" />


</android.support.constraint.ConstraintLayout>
